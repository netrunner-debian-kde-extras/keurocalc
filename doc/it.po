#, no-c-format
msgid "<keycap>S</keycap>"
msgstr "<keycap>S</keycap>"

#. Tag: member
#: index.docbook:468 index.docbook:505
#, no-c-format
msgid "Change sign of the result"
msgstr "Cambia il segno del risultato"

#. Tag: member
#: index.docbook:470
#, no-c-format
msgid "Any other key"
msgstr "Qualsiasi altro tasto"

#. Tag: member
#: index.docbook:471 index.docbook:496
#, no-c-format
msgid "Validation of input number considered as a number in the other currency"
msgstr ""
"Conferma del numero immesso, considerato come un numero nell'altra valuta"

#. Tag: title
#: index.docbook:477
#, no-c-format
msgid "The buttons"
msgstr "I pulsanti"

#. Tag: member
