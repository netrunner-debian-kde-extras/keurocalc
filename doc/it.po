msgid "<guibutton>AC</guibutton>"
msgstr "<guibutton>AC</guibutton>"

#. Tag: member
#: index.docbook:490
#, no-c-format
msgid "Clear all: both input and result area"
msgstr "Pulisci tutto: sia l'immissione che l'area del risultato"

#. Tag: member
#: index.docbook:492
#, no-c-format
msgid "<guibutton>€</guibutton> or <guibutton>$</guibutton>"
msgstr "<guibutton>€</guibutton> o <guibutton>$</guibutton>"

#. Tag: member
#: index.docbook:495
#, no-c-format
msgid "The button labeled with the name of an other currency"
msgstr "Il pulsante indicato con il nome di un'altra valuta"

#. Tag: guibutton
#: index.docbook:498
#, no-c-format
msgid "Enter"
msgstr "Invio"

