#, no-c-format
msgid "&lt;-"
msgstr "&lt;-"

#. Tag: guibutton
#: index.docbook:494
#, no-c-format
msgid "<guibutton>AC</guibutton>"
msgstr "<guibutton>AC</guibutton>"

#. Tag: member
#: index.docbook:495
#, no-c-format
msgid "Clear all: both input and result area"
msgstr "Rensar allt: både inmatnings- och resultatområdet"

#. Tag: member
#: index.docbook:497
#, no-c-format
msgid "<guibutton>€</guibutton> or <guibutton>$</guibutton>"
msgstr "<guibutton>€</guibutton> eller <guibutton>$</guibutton>"

#. Tag: member
#: index.docbook:500
#, no-c-format
msgid "The button labeled with the name of an other currency"
msgstr "Knappen som har namnet på en annan valuta"

#. Tag: guibutton
#: index.docbook:503
#, no-c-format
msgid "Enter"
msgstr "Enter"

