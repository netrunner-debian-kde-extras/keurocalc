#. Tag: keycap
#: index.docbook:454
#, no-c-format
msgid "<keycap>%</keycap>"
msgstr "<keycap>%</keycap>"

#. Tag: member
#: index.docbook:455 index.docbook:507
#, no-c-format
msgid "Validation of input number considered as a percentage"
msgstr "Validación del número de entrada considerado como un porcentaje."

#. Tag: member
#: index.docbook:457
#, no-c-format
msgid "&Backspace;"
msgstr "&Backspace;"

#. Tag: member
#: index.docbook:458 index.docbook:492
#, no-c-format
msgid "Removal of last input: digits, decimal separator or operator"
msgstr "Elimina la última entrada: dígitos, separador decimal u operador."

#. Tag: member
#: index.docbook:460
#, no-c-format
msgid ""
"<keycap>Tab</keycap> and <keycombo action=\"simul\">&Shift; <keycap>Tab</keycap>"
"</keycombo> keys"
msgstr ""
"Teclas <keycap>Tab</keycap> y <keycombo action=\"simul\">&Shift; <keycap>"
"Tab</keycap></keycombo>"

#. Tag: member
#: index.docbook:461
#, no-c-format
msgid "Moves from one button to the next or previous one"
msgstr "Se mueve de un botón al siguiente o al anterior"

#. Tag: member
#: index.docbook:463
#, no-c-format
msgid "Arrow keys"
msgstr "Teclas de flechas"

#. Tag: member
#: index.docbook:464
#, no-c-format
msgid "In the currency selection area, moves from one currency to the other"
msgstr "En el área de selección de moneda, muévase de una moneda a otra."

#. Tag: member
#: index.docbook:466
#, no-c-format
msgid "Space bar"
msgstr "Barra espaciadora"

#. Tag: member
#: index.docbook:467
#, no-c-format
msgid "When focus is on a push button, presses this button"
msgstr "Cuando el foco se encuentra sobre un botón, pulsa este botón."

#. Tag: member
#: index.docbook:469
#, no-c-format
msgid "<keycap>E</keycap> or <keycap>$</keycap>"
msgstr "<keycap>E</keycap> o <keycap>$</keycap>"

#. Tag: member
#: index.docbook:470 index.docbook:498
#, no-c-format
msgid ""
"Validation of input number considered as a value expressed in the reference "
"currency (euro or dollar)"
msgstr ""
"Validación del número introducido considerado como un valor expresado en la "
"moneda de referencia (euro o dólar)."

#. Tag: keycap
#: index.docbook:472
#, no-c-format
msgid "<keycap>S</keycap>"
msgstr "<keycap>S</keycap>"

#. Tag: member
#: index.docbook:473 index.docbook:510
#, no-c-format
msgid "Change sign of the result"
msgstr "Cambia el signo del resultado."

#. Tag: member
#: index.docbook:475
#, no-c-format
msgid "Any other key"
msgstr "Cualquier otra tecla"

#. Tag: member
#: index.docbook:476 index.docbook:501
#, no-c-format
msgid "Validation of input number considered as a number in the other currency"
msgstr ""
"Valida la entrada del número considerado como un número en la otra moneda."

#. Tag: title
#: index.docbook:482
#, no-c-format
msgid "The buttons"
msgstr "Los botones"

#. Tag: member
#: index.docbook:485
#, no-c-format
msgid "<guibutton>0</guibutton> to <guibutton>9</guibutton>"
msgstr "<guibutton>0</guibutton> a <guibutton>9</guibutton>"

#. Tag: member
#: index.docbook:488
#, no-c-format
msgid "<guibutton>.</guibutton> or <guibutton>,</guibutton>"
msgstr "<guibutton>.</guibutton> o <guibutton>,</guibutton>"

#. Tag: guibutton
#: index.docbook:491
#, no-c-format
msgid "&lt;-"
msgstr "&lt;-"

#. Tag: guibutton
#: index.docbook:494
#, no-c-format
msgid "<guibutton>AC</guibutton>"
msgstr "<guibutton>AC</guibutton>"

#. Tag: member
#: index.docbook:495
#, no-c-format
msgid "Clear all: both input and result area"
msgstr "Borra todo: las áreas de entrada y de resultado."

#. Tag: member
#: index.docbook:497
#, no-c-format
msgid "<guibutton>€</guibutton> or <guibutton>$</guibutton>"
msgstr "<guibutton>€</guibutton> o <guibutton>$</guibutton>"

#. Tag: member
#: index.docbook:500
#, no-c-format
msgid "The button labeled with the name of an other currency"
msgstr "El botón etiquetado con el nombre de otra moneda."

#. Tag: guibutton
#: index.docbook:503
#, no-c-format
msgid "Enter"
msgstr "Introducir"

#. Tag: guibutton
#: index.docbook:506
#, no-c-format
msgid "<guibutton>%</guibutton>"
msgstr "<guibutton>%</guibutton>"

#. Tag: guibutton
#: index.docbook:509
#, no-c-format
msgid "<guibutton>+/-</guibutton>"
msgstr "<guibutton>+/-</guibutton>"

#. Tag: member
#: index.docbook:512
#, no-c-format
msgid ""
"<guibutton>Min</guibutton>, <guibutton>MR</guibutton>, <guibutton>M+</guibutton> "
"and <guibutton>-</guibutton>"
msgstr ""
"<guibutton>Min</guibutton>, <guibutton>MR</guibutton>, <guibutton>M+</guibutton> "
"y <guibutton>-</guibutton>"

#. Tag: member
#: index.docbook:513
#, no-c-format
msgid ""
"Store result in memory, recall memory into result, add result to memory and "
"subtract result from memory"
msgstr ""
"Guarda el resultado en memoria, recupera el resultado de la memoria, suma el "
"resultado a la memoria y resta el resultado de la memoria."

#. Tag: member
#: index.docbook:515
#, no-c-format
msgid ""
"<guibutton>/</guibutton>, <guibutton>X</guibutton>, <guibutton>-</guibutton> "
"and <guibutton>+</guibutton>"
msgstr ""
"<guibutton>+</guibutton>, <guibutton>-</guibutton>, <guibutton>X</guibutton> "
"y <guibutton>/</guibutton>"

#. Tag: guibutton
#: index.docbook:518
#, no-c-format
msgid "Reset"
msgstr "Reiniciar"

