"<keycap>+</keycap>, <keycap>-</keycap>, <keycap>*</keycap> and <keycap>"
"/</keycap>"
msgstr ""
"<keycap>+</keycap>, <keycap>-</keycap>, <keycap>*</keycap> y <keycap>/</keycap>"

#. Tag: member
#: index.docbook:449 index.docbook:516
#, no-c-format
msgid "Operators: addition, subtraction, multiplication and division"
msgstr "Operadores: suma, resta, multiplicación y división."

#. Tag: member
#: index.docbook:451
#, no-c-format
msgid "<keycap>Enter</keycap> and <keycap>Return</keycap>"
msgstr "<keycap>Introducir</keycap> y <keycap>Entrar</keycap>"

#. Tag: member
#: index.docbook:452 index.docbook:504
#, no-c-format
