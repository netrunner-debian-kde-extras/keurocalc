#. Tag: title
#: index.docbook:490
#, no-c-format
msgid "The buttons"
msgstr "Os botões"

#. Tag: member
#: index.docbook:493
#, no-c-format
msgid "<guibutton>0</guibutton> to <guibutton>9</guibutton>"
msgstr "<guibutton>0</guibutton> a <guibutton>9</guibutton>"

#. Tag: member
#: index.docbook:496
#, no-c-format
msgid "<guibutton>.</guibutton> or <guibutton>,</guibutton>"
msgstr "<guibutton>.</guibutton> ou <guibutton>,</guibutton>"

#. Tag: guibutton
#: index.docbook:499
#, no-c-format
msgid "&lt;-"
msgstr "&lt;-"

#. Tag: guibutton
#: index.docbook:502
#, no-c-format
msgid "<guibutton>AC</guibutton>"
msgstr "<guibutton>AC</guibutton>"

#. Tag: member
#: index.docbook:503
#, no-c-format
msgid "Clear all: both input and result area"
msgstr "Limpar tudo: tanto os parâmetros como a área de resultado"

#. Tag: member
#: index.docbook:505
#, no-c-format
msgid "<guibutton>€</guibutton> or <guibutton>$</guibutton>"
msgstr "<guibutton>€</guibutton> ou <guibutton>$</guibutton>"

#. Tag: member
#: index.docbook:509
#, no-c-format
msgid "The button labeled with the name of an other currency"
msgstr "O botão assinalado com o nome da outra moeda"

#. Tag: guibutton
#: index.docbook:512
#, no-c-format
msgid "Enter"
msgstr "Enter"

#. Tag: guibutton
#: index.docbook:515
#, no-c-format
msgid "<guibutton>%</guibutton>"
msgstr "<guibutton>%</guibutton>"

#. Tag: guibutton
#: index.docbook:518
#, no-c-format
msgid "<guibutton>+/-</guibutton>"
msgstr "<guibutton>+/-</guibutton>"

#. Tag: member
#: index.docbook:521
#, no-c-format
msgid ""
"<guibutton>Min</guibutton>, <guibutton>MR</guibutton>, <guibutton>M+</"
"guibutton> and <guibutton>-</guibutton>"
msgstr ""
"<guibutton>Min</guibutton>, <guibutton>MR</guibutton>, <guibutton>M+</"
"guibutton> e <guibutton>M-</guibutton>"

#. Tag: member
#: index.docbook:522
#, no-c-format
msgid ""
"Store result in memory, recall memory into result, add result to memory and "
"subtract result from memory"
msgstr ""
"Guarda o resultado na memória, restaura o resultado com o valor em memória, "
"adiciona o resultado à memória ou subtrai-o, respectivamente"

#. Tag: member
#: index.docbook:524
#, no-c-format
msgid ""
"<guibutton>/</guibutton>, <guibutton>X</guibutton>, <guibutton>-</guibutton> "
"and <guibutton>+</guibutton>"
msgstr ""
"<guibutton>/</guibutton>, <guibutton>X</guibutton>, <guibutton>-</guibutton> "
"e <guibutton>+</guibutton>"

#. Tag: guibutton
#: index.docbook:527
#, no-c-format
msgid "Reset"
msgstr "Reset"

#. Tag: member
#: index.docbook:528
#, no-c-format
msgid "Reset both input, result and memory"
msgstr "Limpar o valor inserido, o resultado e a memória"

#. Tag: guibutton
#: index.docbook:530
#, no-c-format
msgid "About"
msgstr "Sobre"

#. Tag: member
